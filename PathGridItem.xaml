<UserControl x:Class="FSSY_v3.PathGridItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:FSSY_v3"
             mc:Ignorable="d">
    <Grid>
        <Grid Style="{StaticResource HoverImageGridStyle}">
            <TextBlock Style="{StaticResource PathNrText}" Text="{Binding PathNumber}" />
            <TextBox Style="{StaticResource PathBox}" Text="{Binding PathText}" />
        </Grid>
    </Grid>
</UserControl>